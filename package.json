{
  "name": "todolist",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start-b\" \"npm run start-f\"",
    "start-f": "ng serve --proxy-config proxy.conf.dev.json",
    "start-b": "node src-server/index",
    "start-bw": "nodemon src-server/index.js --watch src-server",
    "tslint": "tslint src/**/*.ts src-server/**/*.ts --project tslint.json -t verbose",
    "tslint-fix": "tslint --fix src/**/*.ts src-server/**/*.ts --project tslint.json -t verbose",
    "compile": "tsc --project tsconfig.json",
    "compile-w": "tsc --watch --project tsconfig.json",
    "build": "ng build",
    "lint": "ng lint"
  },
  "keywords": [],
  "author": "mike",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "bcryptjs": "2.4.3",
    "core-js": "^2.5.4",
    "hapi": "17.4.0",
    "hapi-auth-jwt2": "^8.1.0",
    "mongoose": "5.0.16",
    "nconf": "0.10.0",
    "rxjs": "~6.2.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6",
    "zone.js": "~0.8.26",
    "joi": "13.2.0",
    "nodemon": "1.18.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/bcryptjs": "2.4.1",
    "@types/hapi": "17.0.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/joi": "13.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "concurrently": "^4.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
